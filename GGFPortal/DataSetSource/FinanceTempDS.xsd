                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT x.site, x.開航日, x.客戶, x.ord_nbr, x.style_no, x.ord_seq, x.PO_NUMBER, x.vendor_name_brief, x.出貨數量, x.出貨金額, x.代理商, x.open_date, x.vendor_id, y.pre_qty, x.出貨數量 - y.pre_qty AS 差異量, x.estimate_ie FROM (SELECT a.site, CASE WHEN open_date IS NULL THEN '' ELSE CONVERT (varchar(8) , open_date , 112) END AS 開航日, c.agents AS 客戶, a.ord_nbr, c.cus_item_no AS style_no, b.ord_seq, b.cust_po_nbr AS PO_NUMBER, d.vendor_name_brief, SUM(b.sale_qty) AS 出貨數量, SUM(b.shp_amt) AS 出貨金額, c.cus_id AS 代理商, a.open_date, c.vendor_id, c.estimate_ie FROM shpc_bah AS a LEFT OUTER JOIN shpc_bat AS b ON a.site = b.site AND a.kind = b.kind AND a.shp_nbr = b.shp_nbr LEFT OUTER JOIN ordc_bah1 AS c ON a.ord_nbr = c.ord_nbr AND a.site = c.site LEFT OUTER JOIN bas_vendor_master AS d ON c.vendor_id = d.vendor_id AND a.site = d.site WHERE (a.head_status = 'CL') AND (b.detail_status = 'CL') GROUP BY a.site, b.ord_seq, c.agents, c.cus_item_no, b.cust_po_nbr, d.vendor_name_brief, c.cus_id, c.vendor_id, a.open_date, a.ord_nbr, c.estimate_ie) AS x LEFT OUTER JOIN ordc_bat AS y ON x.ord_nbr = y.ord_nbr AND x.site = y.site AND x.ord_seq = y.ord_seq WHERE (x.site LIKE @site) AND (x.開航日 BETWEEN @Date1 AND @Date2) AND (x.vendor_id LIKE @vendor_id) ORDER BY x.ord_nbr, x.ord_seq</CommandText>
